<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Page Object Model Basics" preserve-order="true"
	parallel="false">

	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener
			class-name="org.uncommons.reportng.JUnitXMLReporter" />
		<listener class-name="com.w2a.listeners.CustomListeners" />
	</listeners>

	<test thread-count="5" name="Login Test">
		<classes>
			<class name="com.w2a.testcases.LoginTest" />
		</classes>
	</test> <!-- Test -->
	
		<test thread-count="5" name="Create Account Test">
		<classes>
			<class name="com.w2a.testcases.AccountCreationTest" />
		</classes>
	</test> <!-- Test -->


</suite> <!-- Suite -->
